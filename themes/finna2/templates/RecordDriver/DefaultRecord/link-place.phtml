<?php
// Use echo to ensure we don't return a line feed
$searchRoute = $this->searchAction ?: $this->searchOptions($this->driver->getSearchBackendIdentifier())->getSearchAction();
echo $this->url(
    $searchRoute,
    [],
    [
        'query' => [
            'lookfor' => '"' . $this->lookfor . '"',
            'type' => 'geographic',
        ],
    ]
);
